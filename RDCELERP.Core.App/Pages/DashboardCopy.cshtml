<script>
    $(document).ready(function () {
        $('#sidemenu-main li').each(function () {
            if (window.location.pathname.includes($(this).attr('data-val'))) {
                $(this).addClass('active');
            }
            else {
                $(this).removeClass('active');
            }
        });
    });

</script>