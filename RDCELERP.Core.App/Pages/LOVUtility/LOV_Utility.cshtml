    <script type="text/javascript">
        //City Datatable
        function CityDT(url) {
            $('#CityDatatable').dataTable({
                "order": [[0, 'desc']],
                "processing": true,
                "serverSide": true,
                "scrollX": true,
                "searching": true,
                "filter": true,
                "dom": 'Bfrtlp',
                "buttons": [
                    {
                        extend: 'excel', text: '<div class="exportbtnwrapper"><button class="btn btn-primary" id="exportbtn"><i class="fa-solid fa-download"></i> Download Excel</button></div>', id: "exportbtn",
                        title: 'City',
                        action: newexportaction,
                        exportOptions: {
                            modifier: {
                                // DataTables core
                                order: 'index',  // 'current', 'applied', 'index',  'original'
                                page: 'all',      // 'all',     'current' for export all pages data
                                search: 'none'     // 'none',    'applied', 'removed'
                            },
                            columns: ':not(.notexport)'
                        }
                    },
                ],
                "oLanguage": {
                    "sEmptyTable": "No City found"
                },
                
                "info": true,
                "paging": true,
                "oLanguage": {
                    "sEmptyTable": "No data found"
                },

                "ajax": {
                    "url": url,
                    "datatype": "json",
                    "type": "POST"
                },

                "columnDefs": [{
                    "targets": [0],
                    "visible": false,
                    "searchable": false
                }],
                "responsive": true,
            "columns": [
                { "data": "cityId", "name": "CityId", "autoWidth": true, "orderable": false },
                { "data": "action", "name": "Action", "autoWidth": true, "orderable": false },
                { "data": "name", "name": "CityName", "autoWidth": true, "orderable": true },
                { "data": "cityCode", "name": "CityCode", "autoWidth": true, "orderable": true },
                { "data": "stateName", "name": "StateName", "autoWidth": true, "orderable": true },
                { "data": "isActive", "name": "IsActive", "autoWidth": true, "orderable": true },
                { "data": "createdDate", "name": "CreatedDate", "autoWidth": true, "orderable": true },
            ]
            });
        }

        //All Data Export--
        function newexportaction(e, dt, button, config) {
            var self = this;
