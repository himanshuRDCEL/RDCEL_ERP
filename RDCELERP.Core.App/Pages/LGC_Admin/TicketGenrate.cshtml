        $("#hdnExchangeorderIds").val(searchIDs);
        $("#exportForm").submit();
    });

    //Added by VK
    //var counter = 0;
    $('.select_all').on('click', function () {
        var counter = 0;
        $('.checkboxinput').each(function () {
            if (this.checked) {
                this.checked = false;
            }
        });

        document.querySelectorAll(".select_all").forEach(item => {
            if (item != this) {
                item.checked = false;
            }
        });

        var parentTable = $(this).closest('.dataTables_wrapper').attr('id');
        if (this.checked) {
            $('#' + parentTable + ' .checkboxinput').each(function () {
                if (counter < 10) {
                    this.checked = true;
                    counter++;
                }
            });
        } else {
            $('#' + parentTable + ' .checkboxinput').each(function () {
                this.checked = false;
                counter-1;
            });
        }
        OnCheckBoxCheck();
    });
    //new
    function OnCheckBoxCheck() {
        counter = 0;
        $('.checkboxinput').each(function () {
            if (this.checked) {
                counter++;
            }
            else {
                counter-1;
            }
            if (counter > 10) {
                alert("You can only select up to 10 Orders.");
                $(this).prop('checked', false);
                return false; // Prevent further action
            }
        });
        var flag = counter > 0;
        if (flag) {
            $('#exportbtn').prop('disabled', false);
        } else {
            $('#exportbtn').prop('disabled', true);
        }
    }

    function BtnDisableOnAction(){
        $('#exportbtn').prop('disabled', true);
