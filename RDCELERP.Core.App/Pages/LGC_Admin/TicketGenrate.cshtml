                "processing": true,
                "serverSide": true,
                "filter": true,
                "scrollX": true,
                "paging": true,
                "searching": false,
                "dom": 'Bfrtlp',
                "buttons": [
                    {
                        text: '<button class="btn btn-primary datatablebtn" id="filterboxbtnright"><i class="fa-solid fa-filter"></i></button>',
                        action: function (e, dt, node, config) {
                            $('.filters').toggle();
                        }
