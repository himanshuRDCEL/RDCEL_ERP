@page
@model RDCELERP.Core.App.Pages.PinCode.DeletPinCodeById

@{
    ViewData["Title"] = "Delete";
    Layout = "_Layout";
}

<div class="row content-area">
    <!--For Lead details-->
    <div class="col-12">
        <div class="box">
            <hr />
            <dl class="row">
                <h5>
                    Are you sure you want to delete <b>@Html.DisplayFor(model => model.TblPinCode.ZipCode)</b>
                </h5>
            </dl>
            <form method="post">
                <input type="hidden" asp-for="TblPinCode.Id" />
                <input type="hidden" asp-for="TblPinCode.Location" />
                <input type="hidden" asp-for="TblPinCode.CreatedBy" />
                <input type="hidden" asp-for="TblPinCode.CreatedDate" />
                <input type="hidden" asp-for="TblPinCode.ModifiedBy" />
                <input type="hidden" asp-for="TblPinCode.ModifiedDate" />
                <input type="hidden" asp-for="TblPinCode.IsActive" />
                <input type="submit" value="Delete" class="btn btn-danger" /> |
                <a asp-page="./Index" class="btn btn-primary mainshadow">Back to List</a>
            </form>
        </div>
    </div>
</div>

