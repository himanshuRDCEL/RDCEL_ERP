                                    <label for="">ActionUrl <span style="color:red;">*</span></label>
                                    <input asp-for="TblAccessList.ActionUrl" class="form-control" />
                                    <span asp-validation-for="TblAccessList.ActionUrl" class="text-danger"></span>
                                </div>
