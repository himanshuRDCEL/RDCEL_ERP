<script type="text/javascript">

    $(function () {
        $('.checkboxinput').click(function () {
            var flag = false;
            $('.checkboxinput').each(function () {
                if (this.checked) {
                    flag = true;
                }
            });
            if (flag) {
                $('#exportbtn').prop('disabled', false);
            }
            else {
                $('#exportbtn').prop('disabled', true);
            }
        });
    });

    function ReopenConfirmId(_Id) {

        $("#hdnId").val(_Id);
        $('#Reopen-conformation').modal('show');
    }

    function ReopenConfirm() {

        var searchIDs = "";
        $('.checkboxinput').each(function () {
            if (this.checked) {
                searchIDs += this.value + ",";
            }
        });

        if (searchIDs != "") {
            searchIDs = searchIDs.replace(/,\s*$/, "");
            $("#hdnId").val(searchIDs);
            Swal.fire({
                title: "Are you sure?",
                text: "You want to Reopen this Order!",
                icon: 'warning',
                showCancelButton: true,
                confirmButtonText: 'Ok',
            }).then((result) => {
                if (result.isConfirmed) {
                    $('#Reopen-conformation').modal('show');
                }
                else {
                    location.reload(true);
                }
                customClass: {
                    confirmButton: 'btn btn-dark'
                }
            });
        }
        else {
            Swal.fire({
                title: "Failed!",
                text: "Please Select any order to Reopen",
                icon: 'warning',
                confirmButtonText: 'Ok',
                customClass: {
                    confirmButton: 'btn btn-dark'
                }
            });
        }

        // searchIDs = searchIDs.replace(/,\s*$/, "");
        // $("#hdnId").val(searchIDs);
        // $('#Reopen-conformation').modal('show');
    }
</script>


