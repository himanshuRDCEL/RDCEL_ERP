    @*var data = "{'orderStartDate': " + orderStartDate + ",'orderEndDate': " + orderEndDate + ",'resheduleStartDate': " + resheduleStartDate + ",'resheduleEndDate': " + resheduleEndDate + ",'productCatId': " + productCatId + ",'productTypeId':" + productTypeId + ",'phoneNo': " + phoneNo + ",'custCity': " + custCity+"}";*@

            var data = '{"orderStartDate": ' + orderStartDate + ',"orderEndDate": ' + orderEndDate + ',"resheduleStartDate": ' + resheduleStartDate + ',"resheduleEndDate": ' + resheduleEndDate + ',"companyName": ' + companyName + ',"productCatId": ' + productCatId + ',"productTypeId":' + productTypeId + ',"phoneNo": ' + phoneNo + ',"custCity": ' + custCity + '}';

        //const obj1 = JSON.parse('{"name":"John", "age":30, "city":"New York"}');
        //const obj2 = JSON.parse(data);
        const myJSON = JSON.stringify(data);
        var obj = $.parseJSON(myJSON);
