    @*var data = {
            "orderStartDate": orderStartDate,
            "orderEndDate": orderEndDate,
            "resheduleStartDate": resheduleStartDate,
            "resheduleEndDate": resheduleEndDate,
            "productCatId": productCatId,
            "productTypeId": productTypeId,
            "phoneNo": phoneNo,
            "custCity": custCity
