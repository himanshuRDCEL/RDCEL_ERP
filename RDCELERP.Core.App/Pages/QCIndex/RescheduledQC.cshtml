        // Destroy the existing DataTable
        table.clear();
        table.destroy();
        //Re-Initialize Datatable
        DataTable3(url3, data);

        // Destroy the existing DataTable
        table = $('#QCRescheduleCountTwo').DataTable();
        // Destroy the existing DataTable
        table.clear();
        table.destroy();
        //Re-Initialize Datatable
        DataTable2(url2, data);

        // Destroy the existing DataTable
        table = $('#QCRescheduleCountOne').DataTable();
        // Destroy the existing DataTable
        table.clear();
        table.destroy();
        //Re-Initialize Datatable
        DataTable(url, data);

        $('.filters').hide();
